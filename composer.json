{
  "require-dev": {
    "phpunit/phpunit": "^12.2",
    "friendsofphp/php-cs-fixer": "^3.75",
    "guzzlehttp/guzzle": "^7.9"
  },
  "autoload": {
    "psr-4": {
      "BlockSense\\": "src/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests",
    "cs-fix": "PHP_CS_FIXER_IGNORE_ENV=1 ./vendor/bin/php-cs-fixer fix src/ --rules=@PSR12"
  }
}
